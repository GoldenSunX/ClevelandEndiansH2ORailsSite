<% provide(:title, 'Search') %>
<h1>Search</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag('find', method: 'get') do %>
        <%= label_tag(:name, 'Name') %>
        <%= text_field_tag(:name, nil, class: 'form-control') %>
        <%= submit_tag('Search', class: 'btn btn-primary') %>
    <% end %>
  </div>
</div>