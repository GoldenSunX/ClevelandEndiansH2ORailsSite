<!-- Created (Sam Yinger, 12/1/2016): Made the table that receives the filtered contacts -->
<!-- Updated (Adam Prater 12/1/2016): HTML Formatting-->
<table id="contacts" border="1" align="center" width="100%">
  <tr id="t_header">
    <th>Name</th>
    <th>Phone Number</th>
    <th>Address</th>
    <th>Email Address</th>
    <th>Edit</th>
  </tr>
  <% @contacts.each do |contact| %>
      <tr id="contact-<%= contact.id %>">
        <td><%= contact.name %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.address %></td>
        <td><%= contact.email %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
      </tr>
  <% end %>
</table>
